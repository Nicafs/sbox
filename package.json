  "scripts": {
    "start": "run(){ ./get-env.sh $1 $2 $3 $4 && env-cmd -f .env craco start;}; run",
    "build": "run(){ ./get-env.sh $1 $2 $3 $4 && env-cmd -f .env craco build --prod;}; run",
    "deploy": "run(){ yarn build $1 $2 $3 $4 && firebase deploy -P $1 --only hosting:$1,firestore:rules,firestore:indexes,storage,database;}; run",
    "deploy:ci": "run(){ yarn build $1 $2 $3 $4 && firebase deploy -P $1 --only hosting:$1,firestore:rules,firestore:indexes,storage,database -m $4 --token $5;}; run",
    "start:e2e": "yarn start e2e 1",
    "start:local": "env-cmd -f .env.local craco start",
    "start:dev": "env-cmd -f .env.dev craco start",
    "start:staging": "env-cmd -f .env.staging craco start",
    "start:apresentacao": "env-cmd -f .env.apresentacao.staging craco start",
    "start:portocred:dev": "./get-env.sh dev 0 '' CE_REACT_CORE_PORTOCRED && env-cmd -f .env craco start",
    "start:semear:dev": "./get-env.sh dev 0 '' CE_REACT_CORE_BANCOSEMEAR && env-cmd -f .env craco start",
    "start:dev:ssl": "HTTPS=true SSL_CERT_FILE=./.cert/cert.pem SSL_KEY_FILE=./.cert/key.pem craco start",
    "start:dev:local": "env-cmd -f .env.dev.local craco start",
    "start:e2e:local": "env-cmd -f .env.e2e.local craco start",
    "build:e2e": "env-cmd -f .env.e2e craco build --prod",
    "build:dev": "env-cmd -f .env.dev craco build --prod",
    "build:staging": "env-cmd -f .env.staging craco build --prod",
    "build:prod": "env-cmd -f .env.prod craco build --prod",
    "test": "craco test --maxWorkers=3",
    "test:coverage": "craco test --watchAll=false --coverage --no-cache --logHeapUsage --silent --maxWorkers=2",
    "lint": "eslint --ext .jsx,.js src",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-format && npm run lint",
    "deploy:dev": "yarn build:dev && firebase deploy -P dev --only hosting:dev,firestore:rules,firestore:indexes,storage,database",
    "deploy:staging": "yarn build:staging && firebase deploy -P staging --only hosting:staging,firestore:rules,firestore:indexes,storage,database"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/yup": "^0.28.3",
    "@welldone-software/why-did-you-render": "^4.2.5",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^4.1.0",
    "husky": "^4.3.0",
    "jest-styled-components": "^6.3.1",
    "prettier": "^2.1.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "resolutions": {
    "webpack-dev-server": "3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test --watchAll=false",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
